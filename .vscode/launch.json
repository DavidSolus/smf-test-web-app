{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch SMF",
      "program": "${workspaceFolder}/build/core/index-debug.js",
      "preLaunchTask": "build",
      "skipFiles": [
        "node_modules"
      ],
      "envFile": "${workspaceFolder}/.env",
      "outFiles": [
        "${workspaceFolder}/build/**/*.js"
      ],
      "outputCapture": "std",
      "sourceMaps": true
    }
  ]
}
